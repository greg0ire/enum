{
    "name": "greg0ire/enum",
    "type": "library",
    "description": "work around the missing enum type in php",
    "keywords": [
        "enum",
        "enumeration",
        "Symfony",
        "validator",
        "form"
    ],
    "license": "CC-BY-SA-3.0",
    "authors": [
        {
            "name": "Grégoire Paris",
            "email": "postmaster@greg0ire.fr"
        },
        {
            "name": "Sullivan Sénéchal",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/inflector": "^1.0"
    },
    "conflict": {
        "symfony/form": "<3.2 || >=5.0 <999",
        "symfony/validator": "<3.2 || >=5.0 <999"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^3.0",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/form": "^4.2",
        "symfony/framework-bundle": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/options-resolver": "^4.2",
        "symfony/phpunit-bridge": "^4.2",
        "symfony/translation": "^4.2",
        "symfony/twig-bundle": "^4.2",
        "symfony/validator": "^4.3.6",
        "twig/twig": "^2.4.2"
    },
    "suggest": {
        "symfony/form": "To use enum form type",
        "symfony/validator": "To use enum validator"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-legacy-stable": "3.x-dev",
            "dev-master": "4.x-dev",
            "dev-unstable": "5.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Greg0ire\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greg0ire\\Enum\\Tests\\": "tests"
        }
    }
}
