{
    "name": "greg0ire/enum",
    "type": "library",
    "description": "work around the missing enum type in php",
    "keywords": ["enum", "enumeration", "Symfony", "validator", "form"],
    "license": "CC-BY-SA-3.0",
    "authors": [
        {
            "name": "Grégoire Paris",
            "email": "postmaster@greg0ire.fr"
        },
        {
            "name": "Sullivan Sénéchal",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/inflector": "^1.0"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^1.0",
        "phpunit/phpunit": "^5.6",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0",
        "symfony/form": "^3.2",
        "symfony/framework-bundle": "^3.2",
        "symfony/phpunit-bridge": "^3.3",
        "symfony/twig-bundle": "^3.2",
        "symfony/validator": "^3.2",
        "twig/twig": "^2.0"
    },
    "suggest": {
        "symfony/form": "To use enum form type",
        "symfony/validator": "To use enum validator"
    },
    "conflict": {
        "symfony/form": "<3.2 || >=4.0 <999",
        "symfony/validator": "<3.2 || >=4.0 <999"
    },
    "autoload": {
        "psr-4": {
            "Greg0ire\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greg0ire\\Enum\\Tests\\": "test"
        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-legacy-stable": "2.x-dev",
            "dev-master": "3.x-dev",
            "dev-unstable": "4.x-dev"
        }
    }
}
