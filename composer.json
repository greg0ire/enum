{
    "name": "greg0ire/enum",
    "description": "work around the missing enum type in php",
    "keywords": ["enum", "enumeration"],
    "license": "CC-BY-SA-3.0",
    "authors": [
        {
            "name": "Gr√©goire Paris",
            "email": "postmaster@greg0ire.fr"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "doctrine/inflector": "^1.0"
    },
    "require-dev": {
        "symfony/form": "^2.7 || ^3.0",
        "symfony/validator": "^2.7 || ^3.0",
        "phpunit/phpunit": "~4.1",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0"
    },
    "suggest": {
        "symfony/form": "To use enum form type",
        "symfony/validator": "To use enum validator"
    },
    "conflict": {
        "symfony/form": "<2.7 || >=4.0",
        "symfony/validator": "<2.7 || >=4.0"
    },
    "autoload": {
        "psr-4": {
            "Greg0ire\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Greg0ire\\Enum\\Tests\\": "test"
        }
    },
    "config":{
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
